	<queryString>
		<![CDATA[SELECT C.TELEFONO TELEFONO,
       CO.NOMBRES || ' ' || CO.APELLIDOS NOMBRE_CHOFER,
       C.CLIENTE CLIENTE,
       C.ORIGEN ORIGEN,
       C.DESTINO DESTINO,
       C.VALOR VALOR,
       C.MOVIL MOVIL,
       to_char(C.FECHA_HORA_REGISTRO, 'dd/mm/rrrr hh24:mi:ss') FECHA_HORA_REGISTRO,
       U.NOMBRES || ' ' || U.APELLIDOS OPERADORA
  FROM CARRERAS C, USUARIOS U, CHOFERES CO
 WHERE C.USUARIO_ID = U.CEDULA
   AND C.MOVIL = CO.MOVIL
   AND C.MOVIL = $P{Movil}
   AND C.FECHA_HORA_REGISTRO >= TO_DATE('24/03/2017', 'DD/MM/RRRR')
   AND C.FECHA_HORA_REGISTRO <= TO_DATE('08/04/2017', 'DD/MM/RRRR') + 1
 order by C.FECHA_HORA_REGISTRO asc]]>
	</queryString>
